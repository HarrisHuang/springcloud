server: 
  port: 6004
  compression: 
    enabled: true
  tomcat: 
    uri-encoding: UTF-8

spring: 
  application: 
    name: cloud-paas-admin
  cloud: 
    consul: 
      host: localhost
      port: 8500 
      discovery:
        ip-address: localhost
        prefer-ip-address: true
        tags: huangyan
  redis:
    host: localhost
    port: 6379
  cache:
    ehcache:
      config: classpath:ehcacheConfig.xml
