.wisdomPark-ui-progress {
	padding: 0 20px;
	margin-bottom: 20px;
	font-size: 14px;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList {
	display: flex;
	display: -webkit-flex;
	justify-content: center;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li {
	flex: 1;
	text-align: center;
	position: relative;
}
/* -----进度条----- */
.wisdomPark-ui-progress .wisdomPark-ui-progressList>li>.line {
	position: relative;
	display: block;
	margin: 25px 0;
	height: 6px;
	background: #fff;
	content: "";
	overflow: hidden;
	z-index: 5;
	box-shadow: 0px 4px 6px rgba(202, 202, 202, 0.5);
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li:first-child>.line
	{
	max-width: 50%;
	left: 50%;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li:last-child>.line
	{
	max-width: 50%;
}
/* -----节点----- */
.wisdomPark-ui-progress .wisdomPark-ui-progressList>li>.dot {
	position: absolute;
	width: 24px;
	height: 24px;
	left: 50%;
	top: 16px;
	margin-left: -12px;
	background: #fff;
	border-radius: 50%;
	z-index: 10;
	box-shadow: 0px 4px 6px rgba(202, 202, 202, 0.5);
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li>.dot:before {
	content: "";
	display: block;
	margin: 7px;
	width: 10px;
	height: 10px;
	background: #878787;
	border-radius: 50%;
	box-shadow: 0px 0px 6px rgba(202, 202, 202, 0.5);
}

.wisdomPark-ui-progress .title {
	color: #878787;
	padding-top: 10px;
	line-height: 1;
}
/* active */
.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.active>.line,
	.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.active>.dot {
	background: #ea2929;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.active>.dot:before
	{
	background: #fff;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.ing:after,
	.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.ing:before {
	content: "";
	position: absolute;
	left: 50%;
	top: 0;
	margin-left: -28px;
	width: 56px;
	height: 56px;
	border-radius: 50%;
	background: rgba(234, 41, 41, 0.3);
	z-index: 1;
	transform: scale(1);
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.ing:after {
	-webkit-animation: warn 3s infinite ease-out;
	animation: pulse 2s infinite ease-out;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList>li.ing:before {
	-webkit-animation: warn 3s infinite ease-out;
	animation: pulse1 2s infinite ease-out;
}

.wisdomPark-ui-progress .wisdomPark-ui-progressList li.ing .title {
	color: #ea2929;
}
/*********下拉框*********/
.wisdomPark-ui-chevronDown [data-toggle="collapse"] {
