/* ui-grid内表格样式 */
.tables .ui-grid-row [ui-grid-row="row"] {
	display: flex;
	display: -webkit-flex;
	align-items: stretch;
}

.tables .ui-grid-contents-wrapper .ui-grid-render-container [class*="ui-grid-header-cell"] .ui-grid-cell-contents
