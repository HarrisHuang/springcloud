	border: 0;
}

.border1 {
	border: 1px solid #e0e0e0;
}

.border-l1 {
	border-left: 1px solid #ccc;
}

.border-t1 {
	border-top: 1px solid #ccc;
}

.border-r1 {
	border-right: 1px solid #ccc;
}

.border-b1 {
	border-bottom: 1px solid #ccc;
}
/*------------浮动--------------*/
.clean {
	clear: both;
}

.fl {
	float: left;
}

.fr {
	float: right;
}
/*------------内间距--------------*/
.pd_r0 {
	padding-right: 0 !important;
}

.pd_l0 {
	padding-left: 0 !important;
}

.pd_t0 {
	padding-top: 0;
}

.pd_b0 {
	padding-bottom: 0;
}

.pd0 {
	padding: 0;
}

.pd10 {
	padding: 10px;
}

.pd20 {
	padding: 20px
}

.pd15 {
	padding: 15px
}

.pd-top-20 {
	padding-top: 20px;
}

.pd-top-20 {
	padding-top: 20px;
}

.pd-bottom-20 {
	padding-bottom: 20px;
}

.pd-left-20 {
	padding-left: 20px;
}

.pd-right-20 {
	padding-right: 20px;
}
/*------------外间距--------------*/
.mg0 {
	margin: 0;
}

.mg5 {
	margin: 5px;
}

.mg_r8 {
	margin-right: 8px;
}

.mg_l8 {
	margin-left: 8px;
}

.mg15 {
	margin: 15px;
}

.mg20 {
	margin: 20px;
}

.mg_t15 {
	margin-top: 15px
}

.mg_t20 {
	margin-top: 20px
}

.mg_b15 {
	margin-bottom: 15px
}

.mg_b20 {
	margin-bottom: 20px
}

.mg_l15 {
	margin-left: 15px
}

.mg_l20 {
	margin-left: 20px
}

.mg_r15 {
	margin-right: 15px
}

.mg_r20 {
	margin-right: 20px
}
/*------------overflwo hidden--------------*/
.wrapper {
	overflow: hidden;
}
/*------------圆角--------------*/
.radius {
	border-radius: 4px !important;
}

.radius10 {
	border-radius: 10px;
}
/*------------阴影--------------*/
.shadow {
	box-shadow: 0px 0px 5px 1px rgba(00, 00, 00, 0.35);
}

.shadow50 {
	box-shadow: 0px 0px 50px 1px rgba(00, 00, 00, 0.35);
}
/*------------流程进度--------------*/
.fuelux .wizard {
	background-color: #fff;
	border: none;
	-webkit-border-radius: 0px;
	-moz-border-radius: 0px;
	border-radius: 0px;
	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	box-shadow: none;
}

.fuelux .wizard ul li {
	padding: 0 40px 0 50px;
	color: #333;
	background-color: rgb(240, 240, 240);
}

.fuelux .wizard ul li.active {
	color: #fff;
	background-color: #e63636;
}

.fuelux .wizard ul li .badge {
	color: #fff;
	background-color: rgb(125, 125, 130);
}

.fuelux .wizard ul li.active .badge {
	color: #e63636;
	background-color: rgb(240, 240, 240);
}

.fuelux .wizard ul li .chevron {
	top: -25px;
	border: 48px solid transparent;
	border-right: 0;
	border-left: 24px solid #fff;
}

.fuelux .wizard ul li .chevron:before {
	right: 10px;
	border-left-color: rgb(240, 240, 240);
}

.fuelux .wizard ul li.active .chevron:before {
	border-left-color: #e63636;
}
/*------------弹性布局--------------*/
.flex-box {
	display: -webkit-flex;
	display: flex;
	align-items: center;
}

/*------------面包屑--------------*/
.wisdomPark-ui-breadcrumb {
	margin: 0;
	padding: 0;
}

.wisdomPark-ui-breadcrumb li {
	position: relative;
	display: inline-block;
}

.wisdomPark-ui-breadcrumb li+li:before {
	content: "|";
	display: inline-block;
	position: relative;
	top: -1px;
	padding: 0px 5px;
}

/*==========================================================
  2- layout布局样式（覆盖smart admin默认样式）
  此处样式为整体布局样式
  包括页面背景，顶部菜单栏样式，左边菜单栏样式，面包屑样式，表单样式等
  此处样式大部分为覆盖smart admin默认样式
  如需修改此处样式请联系shi xihong
==========================================================*/
/**********body背景*********/
body {
	overflow-x: hidden;
}

.smart-style-3 body, body.smart-style-3 {
	background-color: #f1f2f4 !important;
}

.smart-style-3 body, body.smart-style-3 {
	background-color: #efefef !important;
}
/**********content*********/
#main {
	margin-left: 15%;
	padding: 0;
	height: 100%;
	position: relative;
}

.router-animation-container {
	background-color: #efefef;
	padding: 20px;
}
/**********顶部菜单栏*********/
#extr-page #header {
	background-color: #e63636 !important;
	background-image: url(../img/portal-web-logo.png);
	margin-bottom: 40px
}

#header {
	display: block;
	height: 68px;
	margin: 0;
	padding: 0 0px 0 0;
	background-color: #e63636 !important;
	background-image: url(../img/portal-web-logo.png) !important;
	background-position: left center;
	background-repeat: no-repeat;
	height: 68px
}

#header>:first-child, aside {
	width: 15%;
}

.smart-style-3 aside {
	background: #ffffff;
	border-right: 1px solid #cbcdd1;
}
/**********顶部菜单栏图标样式*********/
.header-icon {
	color: #fff;
	border-left: 1px #C12222 solid;
	padding: 23px 0;
}

.header-icon a {
	padding: 25px 25px;
}

.header-icon span {
	color: #fff;
}

.header-icon:hover {
	border-color: #C12222 !important;
}

/**********顶部用户名，下拉框以及快速链接*********/
#logo {
	min-width: 15%;
	display: none
}

#logo-group span#activity {
	background: rgba(255, 255, 255, .3);
	color: #fff;
	border: 1px solid transparent
}

.smart-style-3 #logo-group span#activity, .smart-style-3 .btn-header>:first-child>a
	{
	color: #fff;
	text-shadow: none;
}

.right-tool-content {
	display: flex;
	display: -webkit-flex;
	flex-direction: row;
	justify-content: flex-end;
	align-items: center;
}

.custom-login .login-info {
	background-color: rgba(255, 255, 255, 0.2) !important;
	border-color: transparent;
	padding: 0 10px 26px 10px;
}

#logo-group .ajax-dropdown {
	top: 85px;
	border: 0;
	border-radius: 4px;
	box-shadow: 0 0 50px 0 rgba(0, 0, 0, .2);
	right: 60px;
}

#logo-group .ajax-dropdown:after, #logo-group .ajax-dropdown:before {
	right: 175px;
}

.ajax-dropdown:before {
	border-bottom-color: #fff;
}

.custom-login .login-info a span, .custom-login .login-info i {
	color: #fff !important;
}

.custom-login .login-info>span {
	padding: 3px 0 3px 5px;
}

.minified .login-info>span {
	padding: 6px 5px !important;
}

.custom-line {
	border-left: 1px #C12222 solid;
	height: 68px;
	cursor: pointer;
}

img.online {
	border-radius: 50%;
	border-left: 0;
	width: 45px !important;
	height: 45px !important;
	border: 1px #fff solid !important;
}

#hide-menu a, #logo-group span#activity {
	padding: 10px 25px;
	height: 68px;
	margin: 0;
	border-color: transparent !important;
	background: none transparent;
	cursor: pointer !important;
}

#logo-group span#activity {
	padding: 20px 20px 20px 20px;
	border: 0;
	height: 28px;
	background: none transparent !important;
}

.header-icon:hover, #hide-menu a:hover, #hide-menu a:active, #logo-group span#activity:hover,
	.hidden-menu #hide-menu>:first-child>a, .custom-login:hover {
	border-color: transparent;
	box-shadow: none !important;
	background: none rgba(255, 255, 255, 0.2) !important;
}

.custom-login:hover {
	border-color: #C12222 !important;
}

#activity b.badge {
	top: 2px;
	right: 5px;
	background-color: #fff !important;
	color: red;
	width: 20px;
	height: 20px;
	padding: 0;
	line-height: 20px;
	text-align: center;
	box-shadow: none;
}

#activity.active .badge {
	background-color: #fff !important;
}

.minified .login-info img {
	margin-right: 10px;
	margin-left: -10px;
}

#quicklinks, #shortcut {
	position: absolute;
	height: auto;
	width: 100%;
	background-color: #33302F;
	background-color: rgba(0, 0, 0, .85);
	z-index: 907;
	display: none;
	color: #fff;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box
}

#shortcut, #quicklinks {
	width: 400px;
	right: 20px;
	top: 85px;
	left: inherit;
	background-color: #fff !important;
	border: 0;
	border-radius: 4px;
	box-shadow: 0 0 50px 0 rgba(0, 0, 0, .2);
}

#quicklinks .custom-arrow, #shortcut .custom-arrow {
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	border-bottom: 10px solid #E63636;
	width: 0;
	height: 0;
	position: absolute;
	right: 15px;
	top: -10px;
	z-index: 99;
}

#quicklinks ul, #shortcut ul {
	background-color: #fff;
	box-shadow: none;
	padding: 0;
	font-size: 0;
	border-bottom: 0 !important;
}

#quicklinks {
	width: 260px;
	right: 60px;
}

#quicklinks .custom-arrow {
	right: 120px;
	border-bottom: 10px solid #fff;
}

#quicklinks ul {
	width: 100%;
	float: left;
	border-radius: 0 0 4px 4px;
	background-color: #EBEBEB;
	padding-bottom: 20px;
	padding-left: 20px;
}

#quicklinks ul li {
	width: 100%;
	float: left;
	padding: 10px 0 10px 0;
	border-bottom: 1px #e0e0e0 solid;
}

#quicklinks ul li a {
	font-size: 14px;
	color: #7C7C7C;
}

#quicklinks ul li span.custom-delete {
	float: right;
	color: #E62425;
	font-size: 16px;
	margin-right: 20px;
	cursor: pointer;
}

#quicklinks ul li a:hover {
	color: #E62425;
}

.jarvismetro-tile {
	background-color: #fff;
	box-shadow: none;
	color: #7C7C7C;
}

.jarvismetro-tile.big-cubes {
	width: 100px;
	height: 80px;
}

.jarvismetro-tile .iconbox i {
	margin: 0;
	height: auto;
	font-size: 25px;
	padding-top: 10px;
}

.jarvismetro-tile .iconbox span {
	text-align: center;
	font-size: 14px;
	padding-top: 10px;
}

.jarvismetro-tile:hover {
	z-index: 10;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
}

.jarvismetro-tile:hover span {
	color: #E62425;
}

.jarvismetro-tile .iconbox span>span {
	right: 5px;
	bottom: 50px;
	box-shadow: none;
}

#shortcut ul li .jarvismetro-tile {
	margin: 0;
}

#quicklinks .custom-user-info, #shortcut .custom-user-info {
	width: 100%;
	background-color: #E63636;
	border-radius: 4px 4px 0 0;
	padding: 20px;
	display: inline-block;
}

#shortcut .custom-user-info img {
	width: 50px;
	height: 50px;
	border-radius: 50%;
	float: left;
	border: 1px #fff solid;
}

#quicklinks .custom-user-info p, #quicklinks .custom-user-info a,
	#shortcut .custom-user-info p {
	float: left;
	color: #fff;
	font-size: 18px;
	margin-left: 20px;
	padding-top: 15px;
}

#quicklinks .custom-user-info {
	background-color: #fff;
	padding: 20px 20px 10px 20px;
}

#quicklinks .custom-user-info p, #quicklinks .custom-user-info a {
	color: #000;
	padding: 0;
	margin: 0;
}

#quicklinks .custom-user-info p {
	margin-left: 0;
}

#quicklinks .custom-user-info a {
	float: right;
}

#shortcut .custom-user-footer {
	width: 100%;
	border-radius: 0 0 4px 4px;
	padding: 30px 0 5px 0;
	background-color: #fff;
	display: inline-block;
}

#shortcut .custom-user-footer p {
	width: 100%;
	float: left;
	padding: 10px 20px;
}

#shortcut .custom-user-footer p:hover {
	background-color: #fde9e9;
}

#shortcut .custom-user-footer a {
	color: #333;
}

#shortcut .custom-user-footer p:hover a {
	/* color: #E62425; */
	
}

.custom-top-notification .custom-user-info {
	width: 100%;
	overflow: hidden;
}

.custom-top-notification .custom-user-info-header {
	padding: 10px 10px 0 10px;
}

.custom-top-notification .custom-user-info p, .custom-top-notification .custom-user-info a
	{
	font-size: 18px;
	color: #000;
}

.custom-top-notification .custom-user-info p {
	float: left;
}

.custom-top-notification .custom-user-info a {
	float: right;
}

.custom-top-notification .notification-body {
	padding-left: 0;
}
/**********面包屑*********/
#ribbon {
	background-color: #f5f5f5;
	min-height: 40px;
	background-color: #fff;
	border-bottom: 1px solid rgba(203, 205, 209, 1);
	box-shadow: 0px 4px 6px rgba(202, 202, 202, 0.25);
	padding: 0 20px;
	position: relative;
}

#ribbon .breadcrumb, #ribbon .breadcrumb a, #ribbon .breadcrumb li {
	color: #333 !important
}

#ribbon .breadcrumb {
	padding: 20px 0 !important;
	margin-bottom: 0px;
	list-style: 20px;
	background-color: #fff;
	border-radius: 0;
}
/**********左边菜单*********/
#left-panel {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 904;
	padding-top: 68px
}

nav ul ul ul li a {
	padding-left: 48px !important;
}

nav ul ul ul li ul li a {
	padding-left: 64px !important;
}

.smart-style-3 nav>ul>li {
	background-image: none;
	background-color: #fff;
}

.smart-style-3 nav>ul>li>a {
	padding: 21px 10px 20px 11px;
}

.smart-style-3 nav>ul>li>a>i {
	color: #444;
}

.smart-style-3 nav ul li a:hover {
	color: #E62425 !important;
	background-color: #fff !important;
}

.smart-style-3 nav ul li a:hover i {
	color: #E62425 !important;
}

.smart-style-3 nav>ul>li.open {
	border-left: 4px #E62425 solid;
}

.smart-style-3 nav>ul>li>ul {
	margin-top: -20px;
}

.smart-style-3 nav>ul>li.open>ul>li>a {
	padding-left: 33px;
}

.smart-style-3 nav>ul>li>a b {
	top: 20px;
}

.smart-style-3.minified nav ul li.open>a:hover {
	color: #444 !important;
}

.smart-style-3 nav>ul>li.active>a:before {
	top: 18px !important;
}

.contain {
	overflow: hidden;
	margin: 0 0 20px 0;
	padding: 0;
	border: 1px solid #dadada;
	border-radius: 5px;
}
/**********通用表单beta*********/
.contain-form {
	border: 1px solid #dadada;
	border-radius: 5px;
}
/* 	表单模块头部	*/
.contain-form .contain-form-header {
	height: 50px;
	padding: 9px 20px;
	border-bottom: 1px solid #dadada;
	font-weight: bold;
}

/* 	表单身体和底部	*/
.contain-form .contain-form-body, .contain-form .contain-form-footer {
	padding: 20px;
}
/* 	标题	*/
.contain-form .contain-form-title {
	margin: 15px -20px 8px;
	padding: 0 20px;
}

.contain-form .contain-form-title h1 {
	box-sizing: content-box;
	margin: 0;
	font-weight: bold;
	padding-top: 10px;
	border-top: 1px solid #dadada;
}
/* 	表格通用 */
.contain-form .contain-form-body .tab-content {
	padding-top: 15px;
}

.contain-form .contain-form-body .table thead {
	background: -webkit-linear-gradient(top, #f1f1f1, #f9f9f9);
	background: -o-linear-gradient(bottom, #f1f1f1, #f9f9f9);
	background: -moz-linear-gradient(bottom, #f1f1f1, #f9f9f9);
	background: linear-gradient(to top, #f1f1f1, #f9f9f9);
	border: none;
}

.contain-form .contain-form-body .table thead th {
	height: 24px;
	line-height: 24px;
}

.contain-form .contain-form-body .table td, .contain-form .contain-form-body .table th
	{
	vertical-align: middle;
	border: none;
}

.contain-form .contain-form-body .table tbody tr {
	height: 50px;
	border-top: 1px solid #dadada;
}

.contain-form .contain-form-body .table tfoot {
	border-top: 1px solid #dadada;
	background: rgb(248, 248, 248);
}

.contain-form .contain-form-body .row+.row {
	margin-top: 15px;
}
/* 	表单通用 	*/
.contain-form .contain-form-body .form-radio input[type="radio"] {
	margin: 0 1em 0 0;
}

.contain-form .contain-form-body .form-radio label {
	margin-right: 20px;
}

.contain-form .contain-form-body textarea {
	resize: none;
}
/* 	去除label的下边距 	*/
.contain-form .contain-form-body .row div[class*="col"] label {
	margin-bottom: 0px;
}
/* 	有按钮的模块底部居中 	*/
.contain-form .contain-form-footer {
	padding-top: 0;
	text-align: center;
}

.contain-form .contain-form-footer .btn {
	margin-right: 5px;
}
/* 	tab切换专用 	*/
.contain-form .contain-form-body .nav-tabs {
	margin: 0 -20px;
	padding: 0 20px;
}

.contain-form .contain-form-body .nav-tabs li.active>a {
	-webkit-box-shadow: 0 -2px 0 #e63636;
	-moz-box-shadow: 0 -2px 0 #e63636;
	box-shadow: 0 -2px 0 #e63636;
}
/* 对嵌入内容显示滚动条  */
.contain-form .contain-form-body .ui-overBox {
	overflow-x: auto;
	overflow-y: hidden;
	border: 1px solid #ddd;
}
/* 显示SmartAdmin的滚动块 */
.contain-form .contain-form-body .ui-overBox::-webkit-scrollbar-thumb {
	background-color: #ddd;
}

/* 去除表格下边距 */
.contain-form .contain-form-body .table {
	margin-bottom: 0;
}
/* 去除SmartAdmin的渐变 */
.contain-form .contain-form-body .table thead tr {
	background: none;
}
/* 带内部边框线的表格 */
.contain-form .contain-form-body .table.td-bordered tbody td+td,
	.contain-form .contain-form-body .table.td-bordered thead th+th {
	border-left: 1px solid #dadada;
}

.contain-form .contain-form-body .table.td-bordered thead tr+tr {
	border-top: 1px solid #dadada;
}
/*  弹出框  */
.ngdialog {
	z-index: 999;
}
/**********通用表单v1.0*********/
.wisdomPark-ui-btn-group {
	/* 去除inline-block间距 */
	word-spacing: -4px;
}

.wisdomPark-ui-btn-sm {
	/* 小按钮 */
	padding: 0px 10px;
	min-width: 60px;
