	font-size: 13px;
}

.ft14 {
	font-size: 14px;
}

.ft16 {
	font-size: 16px;
}

.ft18 {
	font-size: 18px;
}

.ft20 {
	font-size: 20px;
}

.ft22 {
	font-size: 22px;
}

.ft24 {
	font-size: 24px;
}
/*------------字体对齐--------------*/
.textL {
